{
  "name": "portfolio-competences-informatiques",
  "version": "1.0.0",
  "description": "Portfolio professionnel pour présenter les compétences informatiques avec animations modernes",
  "main": "index.html",
  "scripts": {
    "dev": "live-server --port=3000 --open=/index.html",
    "build": "tailwindcss -i ./assets/styles.css -o ./assets/output.css --watch",
    "build:prod": "tailwindcss -i ./assets/styles.css -o ./assets/output.css --minify",
    "serve": "python3 -m http.server 8000",
    "lint": "eslint assets/*.js",
    "format": "prettier --write ."
  },
  "keywords": [
    "portfolio",
    "informatique",
    "compétences",
    "web",
    "responsive",
    "animations",
    "tailwind",
    "javascript"
  ],
  "author": "Votre Nom",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "live-server": "^1.2.2",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.3.6"
  },
  "dependencies": {
    "aos": "^2.3.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/votre-username/portfolio-competences.git"
  },
  "bugs": {
    "url": "https://github.com/votre-username/portfolio-competences/issues"
  },
  "homepage": "https://votre-username.github.io/portfolio-competences"
}
